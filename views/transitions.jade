    .pt-perspective {
      position: relative;
      width: 100%;
      height: 100%;
      perspective: 1200px;
      transform-style: preserve-3d;
    }
    
    .pt-page {
      width: 100%;
      height: 100%;
      position: absolute;
      z-index:100;
      top: 0;
      left: 0;
      visibility: hidden;
      backface-visibility: hidden;
      transform: translate3d(0, 0, 0);
    }
    
    .pt-page-current,
    .no-js .pt-page {
      visibility: visible;
      z-index:200;
    }
    
    .no-js body {
      overflow: auto;
    }
    
    .pt-page-ontop {
      z-index: 999;
    }
    .list-group-item:last-child {
    border-bottom: 2px solid #c2c2c2;
    }
    .pager li>a, .pager li>span {
    display: inline-block;
    padding: 5px 14px;
    background-color: #4F4F4F;
    border:0px; 
    border-radius: 15px;
    }
